select * from pracownicy
where wiek > 30;

select * from pracownicy
where wiek < 30;

select* from pracownicy
where Nazwisko like "K%ki";

alter table pracownicy
change id nr INT;

select * from pracownicy
where imie is null 
or nazwisko is null 
or wiek is null
or kurs is null;

select * from pracownicy
where kurs = "java";



