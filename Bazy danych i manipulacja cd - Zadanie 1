CREATE DATABASE Pracownicy;

create table pracownicy (
ID INT PRIMARY KEY AUTO_INCREMENT,
Imie varchar(50),
Nazwisko varchar(50),
Wiek tinyint unsigned not null check (Wiek BETWEEN 0 AND 120),
Kurs varchar(50)
);

