select * from pracownicy
where wiek = 28;

select * from pracownicy 
where wiek <=30;

select * from pracownicy
where nazwisko like "%ski";

select * from pracownicy
where nr in (1,4,7,18,20);

select * from pracownicy
where imie is not null
or nazwisko is not null
or wiek is not null
or kurs is not null;

select * from pracownicy
where Kurs not like "%DS"
or not null;

