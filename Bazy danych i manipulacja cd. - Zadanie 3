select * from pracownicy
where Nazwisko = "KOWALCZYK";

select * from pracownicy
where wiek between 30 and 40;

select * from pracownicy
where Nazwisko not like "%and%";

select * from pracownicy
where id between 1 and 7;

select * from pracownicy
where imie is null
or nazwisko is null
or wiek is null
or kurs is null;

select * from pracownicy
where kurs is null;
